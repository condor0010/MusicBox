;start
.386
.model flat,stdcall
.stack 4096
ExitProcess proto,dwExitCode:dword

.data
SUNDAY = 0
MONDAY = 1
TUESDAY = 2
WEDNESDAY = 3
THURSDAY = 4
FRIDAY = 5
SATERDAY = 6

week BYTE SUNDAY,NOMDAY,TUESDAY,WEDNESDAY,THURSDAY,FRIDAY,SATERDAY
ininitialized BYTE 5000 DUP(?)
week BYTE 
.code
main proc
	mov eax, 3
	mov ebx, 4
	mov ecx, 5
	mov edx, 6
	add eax, ebx
	add ecx, edx
	sub eax, ecx
	invoke ExitProcess,0
main endp
end main
