;start
.386
.model flat,stdcall
.stack 4096
ExitProcess proto,dwExitCode:dword

.data
val1 WORD 1000h
val2 WORD 2000h
arrrayB BYTE 10h,20h,30h,40h,50h
arrayW word 100h,200h,300h
arrayD DWORD 10000h,20000h


 
.code
main proc

;dem movzx ins
mov bx, 0A69Bh
movzx eax,bx
movzx edx, b1
movzx cx, b1

; demo movsx ins
mov bs, 0a69Bh
movsx eax, bx
movsx edx, b1
movsx edx, b1
mov b1, 7Bh
movsx cx,b1
; m2m ex
mov ax, val1

xchg ax,val1
moov val1, ax

mov al, arrayB
mov al, [arrayB+1]
mov al,[arrayW+2]

mov eax, arrayD
mov eax, [arrayD+4]
mov eax, [arrayD+4]
invoke ExitProcess,0

main endp
end main
